{
    "env": {
        "browser": true,
        "es6": true,
        "jest": true
        },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
        },
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
        },
    "ignorePatterns": ["dist", "coverage"],
    "rules": {
         "prefer-destructuring": "off",
         "no-restricted-syntax": "off",
         //"no-undef": "off"
        }
    }